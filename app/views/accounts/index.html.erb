<h1>Accounts</h1>

<ul>
  <% @accounts.each do |account| %>
    <li><%= link_to account.name, account %></li>
    <%= link_to  "Edit Account", edit_account_path(account) %>
    <%= link_to  "Delete Contact", account, method: :delete %>
  <% end %>
</ul>

<%= link_to  "Open Account", new_account_path %>
<%= button_to  "Log Out", destroy_user_session_path, method: :delete, class: "ui basic negative button" %>
